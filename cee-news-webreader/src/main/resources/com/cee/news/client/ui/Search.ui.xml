<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with type="com.cee.news.client.ui.Resources" field="res" />
	<g:HTMLPanel styleName="{res.styles.searchField}">
		<g:TextBox ui:field="textBoxSearch" styleName=""/>
		<g:Button ui:field="buttonSearch" title="Search" styleName="{res.styles.icon} {res.styles.lense}"/>
		<g:Button ui:field="buttonClear" title="Clear Search" styleName="{res.styles.icon} {res.styles.delete}"/>
	</g:HTMLPanel>
</ui:UiBinder> 
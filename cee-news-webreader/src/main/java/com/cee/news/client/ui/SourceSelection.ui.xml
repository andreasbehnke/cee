<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p="urn:import:com.cee.news.client.ui">
	<ui:with type="com.cee.news.client.ui.Resources" field="res" />
	<g:HTMLPanel styleName="{res.styles.left} {res.styles.sources}">
		<div class="{res.styles.label}">Filter Sources:</div>
		<p:VerticalScrollPanel ui:field="scrollPanel" />
		<g:Button ui:field="buttonSelectAll" styleName="{res.styles.icon} {res.styles.icon24} {res.styles.selectAll}" title="Select All" />
		<g:Button ui:field="buttonSelectNone" styleName="{res.styles.icon} {res.styles.icon24} {res.styles.selectNone}" title="Select None" />
		<g:Button ui:field="buttonAddSource" styleName="{res.styles.icon} {res.styles.icon24} {res.styles.add}" title="Add Source" />
	</g:HTMLPanel>
</ui:UiBinder> 